create table movie (                       movie_id serial primary key,                       title varchar(100) not null,                       length int not null,                       show_start_date date,                       show_end_date date);create table hall(                     hall_number serial primary key,                     row_count int,                     seat_count_in_row int);create table seance (                        seance_id serial not null primary key,                        hall_number int references hall(hall_number),                        seance_date date,                        seance_time varchar(5),                        movie_id int references movie(movie_id));create table ticket(                       ticket_id serial primary key ,                       seance_id int references seance(seance_id),                       row_number int,                       seat_number int,                       purchase_date date);